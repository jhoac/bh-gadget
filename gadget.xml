<?xml version="1.0" encoding="UTF-8" ?> 
<Module>
    <ModulePrefs title="Testing"> 
        <Optional feature="google.calendar-0.5.read"/>
    </ModulePrefs> 
    <Content type="html">
        <![CDATA[
            <div>Test Email</div>
            <script type="text/javascript">
                function subscribeEventCallback(calendarEvent) {
                    var out;
                    if (!calendarEvent) {
                        out = "Loading...");
                    } else if (calendarEvent.attendeeCount <= 2) {
                        for (var atteendee in calendarEvent.attendees) {
                            if (attendee.email !== calendarEvent.owner.email) {
                                out = attendee.email;
                            }
                        }
                    }
                    alert(out);
                    document.write(out);
                };

                google.calendar.read.subscribeToEvents(subscribeEventCallback);
            </script>
        ]]>
    </Content> 
</Module>
