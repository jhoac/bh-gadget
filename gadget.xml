<?xml version="1.0" encoding="UTF-8" ?> 
<Module>
    <ModulePrefs title="Testing"> 
        <Optional feature="google.calendar-0.5.read"/>
    </ModulePrefs> 
    <Content type="html">
        <![CDATA[
            <html>
                <body>
                    <div>Test Email</div>
                    <script>
                        function subscribeEventCallback(calendarEvent) {
                            console.log("Clicked");
                            if (!calendarEvent) {
                                console.log("Loading...");
                            } else {
                                for (var i = 0; i < calendarEvent.attendeeCount; i++) {
                                    console.log("Email: " + calendarEvent.attendees[i].email);
                                    document.write(calendarEvent.attendees[i].email + "/n");
                                }
                            }
                        };
                        google.calendar.read.subscribeToEvents(subscribeEventCallback);
                    </script>
                </body>
            </html>
        ]]>
    </Content> 
</Module>
