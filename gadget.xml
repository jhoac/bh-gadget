<?xml version="1.0" encoding="UTF-8" ?> 
<Module>
    <ModulePrefs title="Testing"> 
        <Optional feature="google.calendar-0.5.read"/>
    </ModulePrefs> 
    <Content type="html">
        <![CDATA[
            <div>Test Email</div>
            <script type="text/javascript">
                function subscribeEventCallback(calendarEvent) {
                    console.log("Clicked");
                    
                    var out;
                    if (!calendarEvent) {
                        out = "Loading..."
                        alert(out);
                        document.write(out);
                    } else if (calendarEvent.attendeeCount <= 2) {
                        for (var attendee in calendarEvent.attendees) {
                            console.log(attendee.email);
                            document.write(attendee.email);
                        }
                    }
                };

                google.calendar.read.subscribeToEvents(subscribeEventCallback);
            </script>
        ]]>
    </Content> 
</Module>
