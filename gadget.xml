    <script type="text/javascript">
        function grabEmailTitle() {
            var emails = document.querySelectorAll("div[id*='guestsBody'] div[class*='plain']");
            alert("ID: " + emails);
            return emails[0].title || "The function was hit";
        };
        window.onload = function() {
            document.getElementsByClassName('navbutton')[0].addEventListener("click", function() { alert("Back!") }, false);
